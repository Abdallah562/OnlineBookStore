<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.online_book_store.controller.ManageAccountController"
      spacing="10" alignment="CENTER" prefWidth="400" prefHeight="400">
    <TextField fx:id="usernameField" promptText="Update Username" />
    <TextField fx:id="passwordField" promptText="Update Password" />
    <TextField fx:id="emailField" promptText="Update email" />
    <TextField fx:id="phoneField" promptText="Update Phone" />
    <Button text="Save Changes" onAction="#saveChanges" />
</VBox>
